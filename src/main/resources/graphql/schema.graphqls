type Query {
    findPersonById(id: ID!): PersonReadModel
    findAllPersons: [PersonReadModel]
}

type PersonReadModel {
    id: ID!
    name: String
    lastName: String
    birthDate: String
    phoneNumber: String
    address: AddressReadModel
}

type AddressReadModel {
    street: String
    number: String
    postalCode: String
}